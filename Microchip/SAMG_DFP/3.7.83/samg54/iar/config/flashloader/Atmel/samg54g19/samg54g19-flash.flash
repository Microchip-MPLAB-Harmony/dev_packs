<?xml version="1.0" encoding="iso-8859-1"?>

<flash_device>
  <exe>$TOOLKIT_DIR$\config\flashloader\Atmel\samg54g19\samg54g19-flash.out</exe>
  <flash_base>0x00400000</flash_base>
  <page>1024</page>
  <block>64 0x2000</block>
  <macro>$TOOLKIT_DIR$\config\debugger\Atmel\Trace_SAMG54.dmac</macro>
  <args_doc>[--flash] specifies the flash loader to program the internal flash.
[--boot] is given, device will boot from flash after programming (GPNVM bit set). 
</args_doc>
</flash_device>
